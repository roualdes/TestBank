{
  "name": "TestBank",
  "version": "1.1.0",
  "description": "A database manager for test questions",
  "author": "Edward A. Roualdes",
  "homepage": "https://roualdes.us",
  "license": "BSD-3-Clause",
  "repository": "https://github.com/roualdes/TestBank",
  "main": "main.js",
  "scripts": {
    "testDev": "electron .",
    "testProd": "electron . --noDevServer",
    "dev": "webpack-dev-server --progress --hot --host 0.0.0.0 --config=./webpack.config.js",
    "build": "webpack --progress -p --config webpack.production.config.js",
    "package": "electron-packager --overwrite ./ --out=./builds",
    "test": "snyk test",
    "format": "prettier --single-quote",
    "lint": "eslint \"src/**/*.{js,jsx}\""
  },
  "pre-commit": ["format", "lint"],
  "dependencies": {
    "electron": "^1.6.11",
    "flux": "^3.1.2",
    "immutable": "^3.8.1",
    "js-yaml": "^3.8.4",
    "json-loader": "^0.5.4",
    "mousetrap": "^1.6.1",
    "nearley": "^2.10.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dropzone": "^3.13.3",
    "semantic-ui-react": "^0.70.0",
    "yaml-loader": "^0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "electron-packager": "^8.7.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-loader": "^1.9.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "snyk": "^1.36.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
